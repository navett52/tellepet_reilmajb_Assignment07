                <SelectCommand>
                  <DbCommand CommandType="Text" ModifiedByUser="true">
                    <CommandText>SELECT        SUM(tTransactionDetail.QtyOfProduct) AS Expr1
FROM            tTransaction INNER JOIN
                         tStore ON tTransaction.StoreID = tStore.StoreID INNER JOIN
                         tTransactionDetail ON tTransaction.TransactionID = tTransactionDetail.TransactionID INNER JOIN
